server { 
    listen 8080;
    server_name localhost;
    client_max_body_size 999999999999999999;
    
    location / {
        root ./www;
		index index.html;
        methods GET POST DELETE;
        upload_path /var/www/uploads;
    }

	 location /favicon.ico {
        root ./www;
        index favicon.ico;
        methods GET;
    }
    location /images {

        index gallery.html;
        methods GET;
        upload_path /var/www/media/uploads;
		return /upload/girl.webp;
    }

    location /upload {
        autoindex on;
        root ./www;
        methods GET POST DELETE;
        upload_path upload/;
        index /1.pdf;
        }

	location /cgi-bin {
        autoindex on;
		root ./www;
		cgi_pass /usr/bin/python3; #path to interpreter
		methods GET POST;
        return http://localhost:8080/upload;
	}
}

server {
    listen 8081;
    server_name diffrent_name;
    client_max_body_size 342234;

    location / {
        root /var/www/html;
        index index.html;
        methods GET POST DELETE;
        return http://diffrent.localhost:12345/home;
        upload_path /var/www/html/uploads;
    }

    location /api {
        root /var/www/api;
        index index.py;
        methods GET POST;
        return http://diffrent.localhost:12345/api-redirect;
        upload_path /var/www/api/uploads;
    }

    location /admin {
        root /var/www/admin;
        index dashboard.html;
        methods GET DELETE;
        return http://diffrent.localhost:12345/admin-dashboard;
        upload_path /var/www/admin/uploads;
    }
}
